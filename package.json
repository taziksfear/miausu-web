{
	"name": "miausu-web",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"compile": "npm run build && npm run start",
		"start": "node -r dotenv/config ./build/index.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"dev": "cross-env THEME=skeleton vite dev",
		"build": "cross-env THEME=skeleton vite build",
		"build:modern": "cross-env THEME=modern vite build",
		"build:rocket": "cross-env THEME=rocket vite build",
		"build:wintry": "cross-env THEME=wintry vite build",
		"build:vintage": "cross-env THEME=vintage vite build",
		"build:seafoam": "cross-env THEME=seafoam vite build",
		"build:sahara": "cross-env THEME=sahara vite build",
		"build:hamlindigo": "cross-env THEME=hamlindigo vite build"
	},
